//输入10个整数，分别统计输出正数、负数的个数。



#include <stdio.h>

int main()
{
    int arr[10]={0};
    int a=0;
    int b=0;
    int c=0;
    for(a=0;a<10;a++)
    {
        scanf("%d",&arr[a]);
        if(arr[a]>0)
            b++;
        else if(arr[a]<0)
            c++;
    }
    printf("positive:%d\nnegative:%d",b,c);
    return 0;
}
