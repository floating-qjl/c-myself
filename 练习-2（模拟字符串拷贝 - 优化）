#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <assert.h>

char* my_strcpy(char* arr1, const char* arr2)
{
	assert(arr1 && arr2);
	char* tmp = arr1;
	while (*arr1++ = *arr2++);
	return tmp;
}

int main()
{
	char arr1[] = "###############";
	char arr2[] = "我是傻逼";
	printf("%s\n", my_strcpy(arr1, arr2));
	return 0;
}
