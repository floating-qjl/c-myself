#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int a = 0;
	int b = 0;
	int n = 0;
	int c = 0;
	scanf("%d", &n);
	for (a = 1;a <= n;a++)
	{
		for (c = n-a;c > 0;c--)
		{
			printf(" ");
		}
		for (b = 1;b <= a;b++)
		{						
			printf("%d",a);
			printf(" ");
		}
		printf("\n");
	}
	return 0;
}
