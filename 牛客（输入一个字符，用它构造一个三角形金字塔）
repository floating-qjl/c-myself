#include <stdio.h>

int main()
{
    char a = 0;
    scanf("%c", &a);
    int b = 0;
    int c = 0;
    for (b = 0;b < 5;b++)
    {
        for (c = 0;c < 4 - b;c++)
        {
            printf(" ");
        }
        for (c = 0;c <= b;c++)
        {
            printf("%c ",a);
        }
        printf("\n");
    }
    return 0;
}
