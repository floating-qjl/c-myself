#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>

void my_strcpy(char* arr1, char* arr2)
{
	while (1)
	{
		if (arr2[0] != '\0')
		{
			arr1[0] = arr2[0];
			arr1++;
			arr2++;
		}
		else
		{
			arr1[0] = arr2[0];
			break;
		}
    
	}
}

int main()
{
	char arr1[] = "###############";
	char arr2[] = "******";
	my_strcpy(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
